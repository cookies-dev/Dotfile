{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "newline"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#1c1c1c",
          "foreground": "#eeeeee",
          "leading_diamond": "<#444444>\u256d\u2500</>",
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}",
            "ubuntu": "\uf31c",
            "windows": "\ue70f",
            "wsl": "\ue712",
            "wsl_separator": " on "
          },
          "style": "diamond",
          "type": "os"
        },
        {
          "background": "#1c1c1c",
          "foreground": "#00afff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "style": "full",
            "template": "<#7a7a7a>\ue0b1</> \ue5ff {{ .Path }} "
          },
          "style": "powerline",
          "type": "path"
        },
        {
          "background": "#4e9a06",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#c4a000{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#89d1dc{{ end }}",
            "{{ if gt .Behind 0 }}#4e9a06{{ end }}"
          ],
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_icon": "\uf126 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#1c1c1c",
          "foreground": "#E4F34A",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false,
            "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}"
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "type": "python"
        },
        {
          "background": "#1c1c1c",
          "foreground": "#7FD5EA",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false,
            "template": "\ufcd1{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}"
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "type": "go"
        },
        {
          "background": "#1c1c1c",
          "foreground": "#42E66C",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false,
            "template": "\ue718{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}"
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "type": "node"
        },
        {
          "background": "#1c1c1c",
          "foreground": "#E64747",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false,
            "template": "\ue791{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}"
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "type": "ruby"
        },
        {
          "background": "#1c1c1c",
          "foreground": "#E64747",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false,
            "template": "\ue738{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}"
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b4",
          "type": "java"
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "",
          "foreground": "#ffffff",
          "background": "#00897b",
          "background_templates": ["{{ if gt .Code 0 }}#e91e63{{ end }}"],
          "template": " <#fefefe>\ufbab</> {{ .FormattedMs }} ",
          "properties": {
            "threshold": 500,
            "style": "austin",
            "always_enabled": true
          }
        },
        {
          "type": "exit",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#00897b",
          "background_templates": ["{{ if gt .Code 0 }}#e91e63{{ end }}"],
          "leading_diamond": "",
          "trailing_diamond": "\ue0b4 ",
          "template": "<#193549>\uE0B0</> \uE23A ",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "type": "command",
          "background": "#ffffff",
          "foreground": "#000000",
          "properties": {
            "shell": "python",
            "command": "import get_commit as gc;gc.main()"
          },
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 "
        },
        {
          "background": "#1c1c1c",
          "foreground": "#00C5C7",
          "properties": {
            "template": " {{ .CurrentDate | date .Format }} ðŸ•œ",
            "time_format": "15:04:05"
          },
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "type": "time"
        },
        {
          "background": "#1c1c1c",
          "foreground": "#FFEE58",
          "properties": {
            "root_icon": "\uf2dd",
            "template": "\uf1ae"
          },
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "type": "root"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#444444",
          "leading_diamond": "",
          "properties": {
            "template": "<#>\u2570\u2500</>"
          },
          "style": "powerline",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 1
}
