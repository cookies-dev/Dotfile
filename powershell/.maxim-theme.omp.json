{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "blocks": [
    {
      "type": "newline"
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "foreground": "#eeeeee",
          "background": "#1c1c1c",
          "leading_diamond": "<#444444>\u256d\u2500</>",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31c",
            "windows": "\ue70f",
            "wsl": "\ue712",
            "wsl_separator": " on "
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#00afff",
          "background": "#1c1c1c",
          "template": "<#7a7a7a>\ue0b1</> \ue5ff {{ .Path }} ",
          "properties": {
            "style": "full"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#000000",
          "background": "#4e9a06",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#c4a000{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#89d1dc{{ end }}",
            "{{ if gt .Behind 0 }}#4e9a06{{ end }}"
          ],
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "properties": {
            "branch_icon": "\uf126 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "python",
          "style": "diamond",
          "foreground": "#E4F34A",
          "background": "#1c1c1c",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "go",
          "style": "diamond",
          "foreground": "#7FD5EA",
          "background": "#1c1c1c",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ufcd1{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "node",
          "style": "diamond",
          "foreground": "#42E66C",
          "background": "#1c1c1c",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue718{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "ruby",
          "style": "diamond",
          "foreground": "#E64747",
          "background": "#1c1c1c",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue791{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "java",
          "style": "diamond",
          "foreground": "#E64747",
          "background": "#1c1c1c",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue738{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#00897b",
          "background_templates": [
            "{{ if gt .Code 0 }}#e91e63{{ end }}"
          ],
          "leading_diamond": " \ue0b6",
          "template": " <#fefefe>\ufbab</> {{ .FormattedMs }} ",
          "properties": {
            "always_enabled": true,
            "style": "austin",
            "threshold": 500
          }
        },
        {
          "type": "exit",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#00897b",
          "background_templates": [
            "{{ if gt .Code 0 }}#e91e63{{ end }}"
          ],
          "trailing_diamond": "\ue0b4 ",
          "template": "<#193549>\ue0b0</> \ue23a ",
          "properties": {
            "always_enabled": true
          }
        },
        {
          "type": "command",
          "style": "diamond",
          "foreground": "#000000",
          "background": "#ffffff",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "properties": {
            "command": "import get_commit as gc;gc.main()",
            "shell": "python"
          }
        },
        {
          "type": "time",
          "style": "diamond",
          "foreground": "#00C5C7",
          "background": "#1c1c1c",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": " {{ .CurrentDate | date .Format }} ðŸ•œ",
          "properties": {
            "time_format": "15:04:05"
          }
        },
        {
          "type": "root",
          "style": "diamond",
          "foreground": "#FFEE58",
          "background": "#1c1c1c",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "\uf1ae",
          "properties": {
            "root_icon": "\uf2dd"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "powerline",
          "foreground": "#444444",
          "template": "<#>\u2570\u2500</>"
        }
      ],
      "newline": true
    }
  ]
}
